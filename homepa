<script src="js/jquery.js"></script>
<script src="js/js-form-validator.js"></script>
<!-- <script src="js/mycustom.js"></script> -->

<script>
    $("#_loading").hide();
    $("#__error").hide();
    $("#__success").hide();
    $("#__errorform").hide();
    // Get form handle
    var formHandle = document.querySelector('form[name="demo-form-2"]');
    // Got to validation
    new Validator(formHandle, function(err, res) {
        // some code of success of validation
        return res;
    });

    // OnClick Validate
    function validatefilledIn() {
        var arr = document.getElementsByClassName('form-control');
        for (var i = 0; i < arr.length; i++) {
            if (arr[i].value == "") {
                $(arr[i]).val('').css("border-color", "red");
            } else {
                $(arr[i]).css("border-color", "#e1e1e1");
            }
        }
        
        // Radio Validation
        var radioButton = document.getElementsByClassName("radioButton")
        $('#_send').click(function() {
            if ($(':radio[name=Q1]:checked, :radio[name=Q2]:checked, :radio[name=Q3]:checked, :radio[name=Q4]:checked, :radio[name=Q5]:checked, :radio[name=Q6]:checked, :radio[name=Q7]:checked, :radio[name=Q8]:checked').length == 8)
                $(radioButton).css("border-color", "#999999");
            else
                $(radioButton).css("border-color", "red");
        });

    }

    // OnFocusOut Validate
    $(document).ready(function() {
        $("input").focusout(function() {
            if ($(this).val() == '') {
                $(this).css('border', 'solid 1px red');
            } else {
                // If it is not blank. 
                $(this).css('border', 'solid 1px green');
            }
        })
        // .trigger("focusout");
    })
</script>

